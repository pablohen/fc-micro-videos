{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "#seedwork/*": ["./@seedwork/*"],
      "#category/*": ["./category/*"]
    },
    "outDir": "./dist",
    "plugins": [
      { "transform": "typescript-transform-paths" },
      { "transform": "typescript-transform-paths", "afterDeclarations": true }
    ]
  },
  "include": ["./src/"]
}
